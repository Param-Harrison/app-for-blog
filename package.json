{
  "name": "paramlabs",
  "description": "Param Harrison's Blog",
  "version": "1.0.0",
  "author": "Param Harrison <paramanantham@live.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.5.1",
    "gatsby": "^2.0.0-rc.0",
    "gatsby-plugin-react-helmet": "^3.0.0-rc.0",
    "gatsby-plugin-styled-components": "^3.0.0-rc.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "styled-components": "^3.4.5"
  },
  "keywords": [
    "gatsby",
    "storybook",
    "jest"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -p 4040",
    "format": "prettier --ignore-path .gitignore --write '**/*.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "start-storybook -p 6060",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.3",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.3",
    "@babel/preset-env": "^7.0.0-rc.3",
    "@babel/preset-react": "^7.0.0-rc.3",
    "@babel/runtime": "^7.0.0-rc.3",
    "@storybook/addon-actions": "^4.0.0-alpha.17",
    "@storybook/addon-links": "^4.0.0-alpha.17",
    "@storybook/addons": "^4.0.0-alpha.17",
    "@storybook/react": "^4.0.0-alpha.17",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "enzyme-to-json": "^3.3.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "prettier": "^1.14.2"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js"
    },
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupTestFrameworkScriptFile": "./setupTests.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "/.cache/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
